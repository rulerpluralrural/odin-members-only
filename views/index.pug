doctype html
html(lang="en")
	head
		meta(charset="UTF-8")
		meta(name="viewport", content="width=device-width, initial-scale=1.0")
		title= title
	body
		if(user)
			ul
				each user in users 
					li #{ user.username } 
			ul
				each item in messages 
					li #{ item.message }
		else 
			h1 Log in first!
